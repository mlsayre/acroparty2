find({ room_id: roomId }).fetch().length === 0) {                            // 123\n          Meteor.call('games.reset', roomId);                                                     // 124\n        } else {                                                                                  // 125\n          Rooms.update({ room_id: roomId }, {                                                     // 126\n            $set: { subround: \"Results\" }                                                         // 127\n          });                                                                                     // 126\n        }                                                                                         // 129\n        delete voteTimer[roomId];                                                                 // 130\n      }, 4000); //35000                                                                           // 131\n    }                                                                                             // 132\n  },                                                                                              // 133\n  'games.results': function gamesResults(roomId) {                                                // 135\n    if (!resultsTimer[roomId]) {                                                                  // 136\n      // one timer only                                                                           //\n      var currentRound = Rooms.findOne({ room_id: roomId }).round;                                // 137\n      if (currentRound === roundsToPlay) {                                                        // 138\n        resultsTimer[roomId] = Meteor.setTimeout(function () {                                    // 139\n          if (Gamedata.find({ room_id: roomId }).fetch().length < 2) {                            // 140\n            // end game after results if not enough players                                       //\n            delete resultsTimer[roomId];                                                          // 141\n            Meteor.call('games.reset', roomId);                                                   // 142\n          } else {                                                                                // 143\n            Rooms.update({ room_id: roomId }, {                                                   // 144\n              $set: { subround: \"Final results\" }                                                 // 145\n            });                                                                                   // 144\n          }                                                                                       // 147\n          delete resultsTimer[roomId];                                                            // 148\n        }, 22000);                                                                                // 149\n      } else {                                                                                    // 150\n        resultsTimer[roomId] = Meteor.setTimeout(function () {                                    // 151\n          if (Gamedata.find({ room_id: roomId }).fetch().length < 2) {                            // 152\n            // end game after results if not enough players                                       //\n            delete resultsTimer[roomId];                                                          // 153\n            Meteor.call('games.reset', roomId);                                                   // 154\n          } else {                                                                                // 155\n            Rooms.update({ room_id: roomId }, {                                                   // 156\n              $set: { round: currentRound + 1,                                                    // 157\n                subround: \"Get ready\" }                                                           // 158\n            });                                                                                   // 156\n          }                                                                                       // 160\n          delete resultsTimer[roomId];                                                            // 161\n        }, 3000); //22000                                                                         // 162\n      }                                                                                           // 163\n    }                                                                                             // 164\n  },                                                                                              // 165\n  'games.finalresults': function gamesFinalresults(roomId) {                                      // 167\n    if (!finalTimer[roomId]) {                                                                    // 168\n      // one timer only                                                                           //\n      finalTimer[roomId] = Meteor.setTimeout(function () {                                        // 169\n        if (Gamedata.find({ room_id: roomId }).fetch().length < 2) {                              // 170\n          // end game after results if not enough players                                         //\n          delete finalTimer[roomId];                                                              // 171\n          Meteor.call('games.reset', roomId);                                                     // 172\n        } else {                                                                                  // 173\n          // set up for new game                                                                  //\n          Games.remove({ room_id: roomId });                                                      // 174\n          Meteor.call('games.init', roomId);                                                      // 175\n        }                                                                                         // 176\n        delete finalTimer[roomId];                                                                // 177\n      }, 25000);                                                                                  // 178\n    }                                                                                             // 179\n    readyTimer = {};                                                                              // 180\n    playTimer = {};                                                                               // 181\n    playStartTimer = {};                                                                          // 182\n    voteTimer = {};                                                                               // 183\n    resultsTimer = {};                                                                            // 184\n  },                                                                                              // 185\n  'games.reset': function gamesReset(roomId) {                                                    // 187\n    Games.remove({ room_id: roomId });                                                            // 188\n    Gamedata.remove({ room_id: roomId });                                                         // 189\n    Rooms.update({ room_id: roomId }, {                                                           // 190\n      $set: { round: 0,                                                                           // 191\n        subround: \"Waiting for players\" }                                                         // 192\n    });                                                                                           // 190\n    readyTimer = {};                                                                              // 194\n    playTimer = {};                                                                               // 195\n    playStartTimer = {};                                                                          // 196\n    voteTimer = {};                                                                               // 197\n    resultsTimer = {};                                                                            // 198\n  },                                                                                              // 199\n  'games.letterFlipFlagOff': function gamesLetterFlipFlagOff(roomId) {                            // 201\n    Games.update({ room_id: roomId }, {                                                           // 202\n      $set: { turnLetters: false }                                                                // 203\n    });                                                                                           // 202\n  }                                                                                               // 205\n});                                                                                               // 38\n                                                                                                  //\nfunction timer(seconds, roomId, statetoactivate) {                                                // 209\n                                                                                                  //\n  Meteor.setTimeout(function () {                                                                 // 211\n    var countdownStartTime = new Date();                                                          // 212\n    if (statetoactivate === \"playtimerstate\") {                                                   // 213\n      Games.update({ room_id: roomId }, {                                                         // 214\n        $set: { playStartTime: countdownStartTime,                                                // 215\n          showAnswerForm: true }                                                